{{define "css"}}
<style type="text/css">
    h1 {
        padding: 0;
        margin: 0;
    }

    body {
        padding: 0;
        margin: 0;
        font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
    }

    #header {
        height: 47px;
        top: 0;
        width: 100%;
    }

    #logo {
        float: left;
        background-color: #4CAF50;
        height: 100%;
    }

    #logo a {
        padding: 13px;
        text-decoration: none;
        color: white;
        font-family: "PT Sans", Helvetica, Arial, sans-serif;
        font-weight: bold;
        line-height: 1.25;
        text-transform: uppercase;
        display: block;
    }

    ul.nav {
        list-style-type: none;
        margin: 0;
        padding: 0;
        overflow: hidden;
        background-color: #5f5d5d;
    }

    ul.nav li {
        float: left;
        border-right: 1px solid #bbb;
    }

    ul.nav li a {
        display: block;
        color: white;
        text-align: center;
        padding: 14px 16px;
        text-decoration: none;
    }

    ul.nav li a:hover {
        background-color: #111;
    }

    ul.nav li.active {
        background-color: #111;
    }

    .dropdown {
        float: left;
        overflow: hidden;
    }

    .dropdown .dropbtn {
        font-size: 16px;
        border: none;
        outline: none;
        color: white;
        padding: 14px 16px;
        background-color: inherit;
        font-family: inherit;
        margin: 0;
    }

    .dropdown-content {
        display: none;
        position: absolute;
        background-color: #5f5d5d;
        min-width: 160px;
        box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.2);
        z-index: 1;
    }

    .dropdown-content a {
        float: none;
        color: black;
        padding: 12px 16px;
        text-decoration: none;
        display: block;
        text-align: left;
    }

    .dropdown-content a:hover {
        background-color: #ddd;
    }

    .dropdown:hover .dropdown-content {
        display: block;
    }

    #content {
        margin: 10px;
    }

    table {
        border-collapse: collapse;
        width: 100%;
    }

    table td, table th {
        border: 1px solid #ddd;
        padding: 8px;
    }

    table tr:nth-child(even) {
        background-color: #f2f2f2;
    }

    table tr:hover {
        background-color: #ddd;
    }

    table th {
        padding-top: 12px;
        padding-bottom: 12px;
        text-align: left;
        background-color: #4CAF50;
        color: white;
    }

    input[type=text], select, textarea {
        width: 100%;
        padding: 12px;
        box-sizing: border-box;
        border: 1px solid #ccc;
        border-radius: 4px;
        resize: vertical;
    }

    label {
        display: inline-block;
    }

    input.invalid {
        border: 2px solid red;
        border-radius: 4px;
    }

    textarea {
        width: 100%;
        height: 150px;
        padding: 12px 20px;
        box-sizing: border-box;
        border: 2px solid #ccc;
        border-radius: 4px;
        background-color: #f8f8f8;
        resize: none;
    }

    select {
        width: 100%;
        border: none;
        border-radius: 4px;
        background-color: #efefef;
    }

    .button {
        background-color: #4CAF50;
        border: none;
        color: white;
        padding: 13px;
        text-align: center;
        text-decoration: none;
        display: inline-block;
        font-size: 14px;
        cursor: pointer;
        border-radius: 4px;
    }

    .button:hover {
        background-color: #45a049;
    }

    .col-25 {
        float: left;
        width: 25%;
    }

    .col-50 {
        float: left;
        width: 48%;
    }

    .pad10 {
        padding: 10px;
    }

    .float-right {
        float: right;
    }

    .clear {
        clear: both;
    }

    .mr-5 {
        margin-right: 5px;
    }

    .mt-10 {
        margin-top: 10px;
    }

    .content {
        max-width: 500px;
        margin: auto;
    }

    #err-content {
        max-width: 500px;
        margin: auto;
    }

    #notfound {
        position: relative;
        height: 70vh;
    }
    #notfound .notfound {
        position: absolute;
        left: 50%;
        top: 40%;
        -webkit-transform: translate(-50%, -40%);
        -ms-transform: translate(-50%, -40%);
        transform: translate(-50%, -40%);
    }
    .notfound {
        max-width: 710px;
        width: 100%;
        padding-left: 190px;
        line-height: 1.4;
    }

    .notfound .notfound-404 {
        position: absolute;
        left: 0;
        top: 0;
        width: 150px;
        height: 150px;
    }

    .notfound h2 {
        font-family: 'Raleway', sans-serif;
        color: #292929;
        font-size: 28px;
        font-weight: 700;
        text-transform: uppercase;
        letter-spacing: 2.5px;
        margin-top: 0;
    }

    .notfound p {
        font-size: 14px;
        font-weight: 400;
        margin-top: 0;
        margin-bottom: 15px;
        color: #333;
    }
    .field-description {
        padding-left:5px;
        font-size: 0.8em;
        color: #969292;
    }

    .dirs ul{
        display:inline-block; 0position: relative; float: left; clear: left;
        margin:.25em;
        padding:0;
        background: pink;
    }

    .dirs ul#tree:before {
        top: 10px;
    }

    .dirs ul:before{
        content:""; position: absolute; z-index: 1;
        top:.25em; right:auto; bottom:0; left: 1.75em;
        margin: auto;
        border-right: dotted black .1em;
        width: 0; height: auto;
    }
    .dirs ul:after{
        content: "-"; position: absolute; z-index: 3;
        top:0; left:-.5em;
        margin:.65em; padding:0;
        width:.8em; height: .8em;
        text-align:center; line-height: .6em; font-size: 1em;
        background: blue;
    }
    .dirs ul>li{
        display: block; position: relative; float: left; clear: both;
        right:auto;
        padding-left: 1em;
        width:auto;
        text-align: center; color:white;
        background: gray;
    }
    .dirs ul>li>input{
        display:block; position: absolute; float: left; z-index: 4;
        margin:0 0 0 -1em; padding:0;
        width:1em; height: 2em;
        font-size: 1em;
        opacity: 0;
        cursor: pointer;
    }
    .dirs ul>li>input:checked~ul:before{
        display: none;
    }
    .dirs ul>li>input:checked~ul:after{
        content: "+"
    }
    .dirs ul>li>input:checked~ul *{
        display: none;
    }
    .dirs ul>li>span{
        display: block; position: relative; float: left; z-index: 3;
        margin:.25em; padding:.25em;
        background: lightblue;
    }
    .dirs ul>li>span:after{
        content: ""; display: block; position: absolute;
        left:-1em; top:0; bottom:0;
        margin: auto .25em auto .25em;
        border-top: dotted black .1em;
        width: .75em; height: 0;

    }

    .dirs ul>li:last-child:before{
        content: ""; display: block; position: absolute; z-index: 2;
        top:1em; left:0; bottom:-.25em;
        width:.75em; height:auto;
        background:lightblue;
    }

    .dirs #tree{
        position: relative; font-family: "Georgia";
    }
    .dirs #tree:before{
        left:.5em;
    }
    .dirs #tree:after{
        display: none;
    }

    /*decoration*/
    .dirs ul,.dirs ul>li:last-child:before{
        background: white;
    }
    .dirs ul>li{
        background: transparent;
    }
    .dirs ul:after{
        background: white;
        color: black;
        border:solid gray 1px;
        border-radius: .1em;
    }
    .dirs ul>li>span{
        border-radius: .25em;
        color: black;
        background: white;
    }
    .dirs ul>li>input~span:before{
        content:""; display: inline-block;
        margin: 0 .25em  0 0;
        width:1em; height: 1em; ;line-height: 1em;
        background: url("data:image/vnd.microsoft.icon;base64,");
        background-repeat:no-repeat;
        background-size:contain;
    }
    .dirs ul>li>input:checked~span:before{
        background-image: url("data:image/vnd.microsoft.icon;base64,");
    }
    /**/
</style>
{{end}}
